@using Microsoft.AspNet.Identity

@model CarService.Web.ViewModels.ServiceCenterViewModel

<br/>
<div class="row">
        <div class="col-md-3">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <h3 class="panel-title">
                    @Html.LabelFor(x=>x.Name)
                    @Model.Name</h3>
                </div>
                <div class="panel-body">
                    <img src="@Model.ImgUrl" class="img-responsive" />
                    @Html.LabelFor(x=> x.StreetAddress)
                    <p>@Model.StreetAddress</p>
                </div>
            </div>
        </div>
</div>

