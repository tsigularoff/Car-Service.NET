@using CarService.Web.Areas.Administration.ViewModels
@model CarServiceCenterViewModel

<h3 class="panel-heading">Add/Edit service center</h3>
<div >
    <div class="form-group">
        @Html.LabelFor(model => model.Name, new { @class = "col-md-5" })
        @Html.EditorFor(model => model.Name)
    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.StreetAddress, new { @class = "col-md-5" })
        @Html.EditorFor(model => model.StreetAddress)        
    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.Location, new { @class = "col-md-5" })
        @Html.EditorFor(model => model.Location)
    </div>
</div>
