@using CarService.Web.Areas.Administration.ViewModels
@model CarServiceViewModel

<h3 class="panel-heading">Add/Edit service center</h3>
<div>
    <div class="form-group">
        @Html.LabelFor(model => model.Name, new { @class = "col-md-5" })
        @Html.EditorFor(model => model.Name)
    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.CarModelName, new { @class = "col-md-5" })
        @Html.EditorFor(model => model.CarModelName)
    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.Year, new { @class = "col-md-5" })
        @Html.EditorFor(model => model.Year)
    </div>
</div>

